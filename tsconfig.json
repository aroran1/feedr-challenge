{
  "compilerOptions": {
    "baseUrl": "./src/client",
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "lib": ["esnext", "dom", "dom.iterable"],
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "target": "es5"
  },
  "include": ["src/client"],
  "exclude": [
    "node_modules"
  ]
}